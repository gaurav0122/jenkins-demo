pipeline{
    agent any
    stages{
        stage("init"){
            steps{
                echo "Hi, This is gaurav from Dev-ops"
                echo "We are starting for testing"
            }
        }
        stage("Build"){
            steps{
                echo "Building simple maven project"
            }
        }
        stage("Deploy"){
            steps{
                echo "Deploy in staging area"
            }
        }
         stage("Deploy Production"){
            steps{
                echo "Deploy in production area"
            }
        }
        stage("Deploy Production next"){
            steps{
                echo "Deploy in production area"
            }
        }
       
    }

}